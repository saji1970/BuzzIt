{
  "expo": {
    "name": "Buzz it",
    "slug": "buzzit",
    "version": "1.0.2",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#E4405F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.buzzit.app",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.buzzit.app",
      "versionCode": 3,
      "compileSdkVersion": 33,
      "targetSdkVersion": 33,
      "adaptiveIcon": {
        "backgroundColor": "#E4405F"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "plugins": [
      "expo-image-picker",
      "expo-av",
      "expo-location",
      "expo-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "8d899ef2-dbd7-4bdb-8dd7-223105a0a736"
      }
    }
  }
}
