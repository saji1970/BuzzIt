{
  "expo": {
    "name": "Buzz it",
    "slug": "buzzit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E4405F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.buzzit.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.buzzit.app",
      "versionCode": 1,
      "compileSdkVersion": 33,
      "targetSdkVersion": 33,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E4405F"
      }
    },
    "plugins": [
      "expo-image-picker",
      "expo-av",
      "expo-location"
    ],
    "extra": {
      "eas": {
        "projectId": "8d899ef2-dbd7-4bdb-8dd7-223105a0a736"
      }
    }
  }
}
